<Page x:Class="LoginPanelApplication.Panels.AdminPanel"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LoginPanelApplication.Panels"
      mc:Ignorable="d" 
       d:DesignHeight="500" d:DesignWidth="900" Background="GhostWhite" x:Name="Adminpanel"
      Title="AdminPanel">

    <Grid VerticalAlignment="Center" Margin="30">
        <StackPanel Margin="50">
            <Button x:Name="btnUserManager" Click="btnUserManager_Click"  Margin="20">User Manager</Button>
            <Button x:Name="btnLogout" Padding="3" Click="btnLogout_Click"  Margin="20">Logout</Button>
        </StackPanel>
    </Grid>
</Page>
